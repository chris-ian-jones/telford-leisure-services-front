{
  "name": "ng-telford-leisure-services",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run format && kill-port 4200 && ng serve",
    "build": "ng build",
    "build:stats": "ng build --source-map",
    "analyze": "source-map-explorer dist/ng-telford-leisure-services/browser/**/*.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.6",
    "@angular/common": "^19.2.6",
    "@angular/compiler": "^19.2.6",
    "@angular/core": "^19.2.6",
    "@angular/forms": "^19.2.6",
    "@angular/platform-browser": "^19.2.6",
    "@angular/platform-browser-dynamic": "^19.2.6",
    "@angular/router": "^19.2.6",
    "canvas-confetti": "^1.9.3",
    "jwt-decode": "^4.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.6",
    "@types/jasmine": "~5.1.7",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "angular-eslint": "^19.3.0",
    "jasmine-core": "~5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "kill-port": "^2.0.1",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.5.4"
  }
}
